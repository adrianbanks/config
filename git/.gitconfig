[user]
    name = Adrian Banks
    email = 
[core]
    autocrlf = true
[alias]
    co = "!f(){ git checkout $1; }; f"
    cob = "!f(){ git checkout -b $1; }; f"
    com = checkout master
	  graph-branch-view = -c core.pager='less -SRF' log --pretty=oneline --abbrev-commit --branches=* --graph --decorate --all --date-order
	  graph-with-time-ago = -c core.pager='less -SRF' log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
    graph-message-on-separate-line = -c core.pager='less -SRF' log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' --all
    locate = "!f(){ git branch --all --contains $1; }; f"
    rc = rebase --continue
    rhm = reset --hard origin/master
    rh = "!f(){ git reset --hard; git clean -f; }; f"
    ru = remote update --prune
    st = status
    standup = log --pretty=format:'%C(yellow)%h%Creset -%Creset %s %Cgreen(%cD) %C(bold blue)<%an>%Creset' --since yesterday
    statis = status
    su = submodule update --init --recursive
    whereis = "!f(){ git branch --all --contains $1; }; f"
[diff]
    ignoreSubmodules = dirty
    tool = kdiff3
    guitool = kdiff3
[rerere]
    enabled = true
    autoupdate = true
[help]
    autocorrect = 1
[color]
    ui = 1
[merge]
    tool = kdiff3
